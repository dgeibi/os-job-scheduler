    "emotion": "^8.0.12",
    "fbjs": "^0.8.16",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "^8.0.12",
    "react-hot-loader": "^4.0.0-beta.12",
    "redux-zero": "^4.8.1"
